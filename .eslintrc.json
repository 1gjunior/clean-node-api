{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "linebreak-style": "off",
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "max-classes-per-file": "off",
        "no-use-before-define":"off",
        "jest/valid-expect": "off",
        "comma-dangle": "off"
    }
}
{
  "name": "clean-node-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/1gjunior/clean-node-api.git",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "yarn run test:staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "Gilberto Junior <junior.siilva182@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  },
  "scripts": {
    "test": "jest --watch --silent",
    "test:staged": "jest --passWithNoTests --findRelatedTests --silent"
  }
}
